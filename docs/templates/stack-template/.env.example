# Copy to .env and customize
PROJECT_NAME=example

# Database (shared Postgres service)
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=example_db
POSTGRES_USER=supabase
POSTGRES_PASSWORD=change_me_strong_db_password

# Supabase secrets (unique per stack)
JWT_SECRET=change_me_jwt_secret
ANON_KEY=change_me_anon_key
SERVICE_ROLE_KEY=change_me_service_role_key

# External URLs
API_EXTERNAL_URL=https://api-example.yourdomain.com
SUPABASE_PUBLIC_URL=https://api-example.yourdomain.com

# Ports (choose to avoid conflicts)
STUDIO_PORT=3003
KONG_HTTP_PORT=8200
KONG_HTTPS_PORT=8643
PGBOUNCER_PORT=6434
